{
  "name": "@wxflows/sdk",
  "version": "2.0.0",
  "description": "The watsonx Flows Engine SDK",
  "type": "module",
  "main": "dist/index.ts",
  "module": "dist/index.ts",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "files": [
    "/src",
    "/dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./langchain": {
      "types": "./dist/lib/langchain/index.d.ts",
      "import": "./dist/lib/langchain/index.js",
      "default": "./dist/lib/langchain/index.js"
    },
    "./openai": {
      "types": "./dist/lib/openai/index.d.ts",
      "import": "./dist/lib/openai/index.js",
      "default": "./dist/lib/openai/index.js"
    },
    "./watsonx": {
      "types": "./dist/lib/watsonx/index.d.ts",
      "import": "./dist/lib/watsonx/index.js",
      "default": "./dist/lib/watsonx/index.js"
    }
  },
  "scripts": {
    "build": "npm run clean && tsc -b",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "clean": "rimraf dist tsconfig.tsbuildinfo"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "author": "samuel hill <sam.hill@ibm.com>",
  "dependencies": {
    "graphql": "^16.8.1",
    "graphql-request": "^6.1.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/chai": "^5.0.0",
    "@types/lodash": "^4.14.202",
    "@types/mocha": "^10.0.8",
    "@types/node": "^20.11.16",
    "chai": "^5.1.1",
    "dotenv": "^16.4.5",
    "mocha": "^10.7.3",
    "rimraf": "^5.0.10",
    "ts-node": "^10.9.2"
  },
  "peerDependencies": {
    "@langchain/core": "^0.3.18",
    "zod": "^3.23.8"
  },
  "gitHead": "6d50ee5982a95ddd5201157836159be1d986a895"
}
