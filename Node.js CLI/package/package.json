{
  "private": "true",
  "name": "wxflows",
  "description": "IBM watsonx Flows Engine CLI",
  "version": "2.0.0",
  "license": "IBM International License Agreement for Early Release of Programs (ILAR)",
  "author": "IBM",
  "homepage": "https://github.com/steprz/watzen.ai",
  "bugs": "https://github.com/steprz/watzen.ai/issues",
  "keywords": [
    "watsonx",
    "Flows Engine"
  ],
  "bin": {
    "wxflows": "./bin/run.js"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@inquirer/prompts": "^7.1.0",
    "@oclif/core": "^4",
    "@oclif/plugin-help": "^6",
    "@stepzen/transpiler": "^0.45.0-beta.2",
    "@wxflows/sdk": "2.0.0",
    "boxen": "^8.0.1",
    "chalk": "^5.3.0",
    "chalk-template": "^1.1.0",
    "debug": "^4.3.7",
    "graphql": "^16.8.1",
    "i18next": "^24.0.2",
    "i18next-cli-language-detector": "^1.1.8",
    "i18next-fs-backend": "^2.6.0",
    "node-fetch": "^3.3.2",
    "stepzen": "^0.46.0-beta.3",
    "tty-table": "^4.2.3",
    "wrap-ansi": "^9.0.0",
    "yauzl": "^3.2.0"
  },
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4",
    "@types/chai": "^4",
    "@types/debug": "^4.1.12",
    "@types/inquirer": "^8.2.10",
    "@types/mocha": "^10",
    "@types/mock-fs": "^4.13.4",
    "@types/node": "^18",
    "@types/sinon": "^17.0.3",
    "@types/yauzl": "^2.10.3",
    "chai": "^4",
    "cross-env": "^7.0.3",
    "eslint": "^8",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "^3",
    "eslint-config-prettier": "^9",
    "glob": "^10.4.5",
    "inquirer": "^8.2.6",
    "lodash": "^4.17.21",
    "mocha": "^10",
    "nock": "^13.5.6",
    "oclif": "^4",
    "prettier": "^3.3.3",
    "shx": "^0.3.3",
    "sinon": "^19.0.2",
    "strip-ansi": "^7.1.0",
    "ts-node": "^10",
    "tsheredoc": "^1.0.1",
    "yaml": "^2.6.0"
  },
  "files": [
    "/bin/run.cmd",
    "/bin/run.js",
    "/bundles",
    "/dist",
    "/locales",
    "/oclif.manifest.json",
    "LICENSE.pdf"
  ],
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "oclif": {
    "bin": "wxflows",
    "dirname": "wxflows",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " ",
    "additionalHelpFlags": [
      "-h"
    ],
    "additionalVersionFlags": [
      "-v"
    ]
  },
  "scripts": {
    "build": "npm run clean && tsc -b",
    "lint": "eslint . --ext .ts",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "oclif manifest && oclif readme",
    "test": "npm run build && mocha --forbid-only \"test/**/*.test.ts\"",
    "test:single:example": "npm run test:single -- test/commands/whoami.test.ts",
    "test:single": "mocha test/_global.test.ts",
    "version": "oclif readme && git add README.md",
    "clean": "shx rm -rf dist tsconfig.tsbuildinfo"
  }
}
